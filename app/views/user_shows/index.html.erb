<h1>UserShows#index</h1>
<p>Find me in app/views/user_shows/index.html.erb</p>
<h2>Welcome, <%= current_user.username %> </h2>
<% @usershows.each do |usershow| %>
  <div>
    <%= usershow.show.title %>
    <%= usershow.show.imdb %>
    <%= usershow.show.id %>
    <%= link_to "view show", user_show_path(usershow.show) %>
  </div>
<% end %>
